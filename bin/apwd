#!/usr/bin/env python

import os
import sys
import autossh
import autossh.hostlookup
import autossh.default

if len(sys.argv) < 2:
    print ""
    print "Usage:"
    print "  apwd [target]"
    print ""
    sys.exit(0)

target = sys.argv[1]

hl = autossh.hostlookup.HostLookup()
hl.loadFromFile(autossh.default.host_entry_file)
ok, info = hl.get(target)
if not ok:
    print "Target not found: '%s' "%(target) 
    sys.exit(1)
print info
