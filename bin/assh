#!/usr/bin/python3

import os
import sys
import yaml
import autossh.ssh

if len(sys.argv) < 2:
    print("")
    print("Usage:")
    print("  assh [target]")
    print("")
    sys.exit(1)

s = autossh.ssh.new(sys.argv[1])
ok, err = s.login()
if not ok:
    print(err)
    sys.exit(1)
s.autowinsize()
s.interact()
